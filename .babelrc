{
	"presets": [
		["@babel/preset-env",{ "useBuiltIns": "usage","corejs": 2 }],
		"@babel/preset-react"
	],
	"plugins": [
		["@babel/plugin-transform-runtime",{"corejs": 2}],
		"@babel/plugin-syntax-dynamic-import",
		["@babel/plugin-proposal-class-properties",{ "loose": false }],
		[
			"import", {
				"libraryName": "antd-mobile",
				"style": "css"
			}
		]
	]
}